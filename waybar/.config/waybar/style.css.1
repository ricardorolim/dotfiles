* {
border: none;
border-radius: 0;
min-height: 0;
font-family: CaskaydiaCove Nerd Font;
font-weight: bold;
font-size: 14px;
padding: 0;
}

window#waybar {
background: rgba(40, 40, 40, 0.8);
border: 2px solid rgba(61, 61, 61, 1.000000);
}

#battery,
#network,
#clock,
#tray,
#pulseaudio {
margin: 4px;
padding: 4px 8px;
border-radius: 8;
}

#workspaces {
background-color: #3d3d3d;
margin: 4px;
border: 2px solid #525252;
border-radius: 8;
}

#workspaces button {
all: initial;
min-width: 0;
box-shadow: inset 0 -3px transparent;
padding: 4px 8px;
color: #e3cc92;
}

#workspaces button.focused {
color: #ebdbb2;
}

#workspaces button.urgent {
background-color: #d65d0e;
}

#network {
/* color: #282828; */
color: #ebdbb2;
}

#clock {
color: #ebdbb2;
}

#tray {
color: #ebdbb2;
}

#battery,
#pulseaudio {
color: #ebdbb2;
}
